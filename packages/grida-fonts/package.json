{
  "name": "@grida/fonts",
  "version": "0.0.0",
  "private": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./google": {
      "types": "./dist/google.d.ts",
      "import": "./dist/google.mjs",
      "require": "./dist/google.js"
    },
    "./fontface": {
      "types": "./dist/fontface.d.ts",
      "import": "./dist/fontface.mjs",
      "require": "./dist/fontface.js"
    },
    "./fontface-dom": {
      "types": "./dist/fontface-dom.d.ts",
      "import": "./dist/fontface-dom.mjs",
      "require": "./dist/fontface-dom.js"
    },
    "./typr": {
      "types": "./dist/typr/index.d.ts",
      "import": "./dist/typr/index.mjs",
      "require": "./dist/typr/index.js"
    },
    "./parse": {
      "types": "./dist/parse/index.d.ts",
      "import": "./dist/parse/index.mjs",
      "require": "./dist/parse/index.js"
    },
    "./parser/worker": {
      "types": "./dist/parser-worker/index.d.ts",
      "import": "./dist/parser-worker/index.mjs",
      "require": "./dist/parser-worker/index.js"
    },
    "./k": {
      "types": "./dist/k.d.ts",
      "import": "./dist/k.mjs",
      "require": "./dist/k.js"
    }
  },
  "scripts": {
    "build": "tsup google.ts fontface.ts fontface-dom.ts typr/index.ts parse/index.ts parser-worker/index.ts parser-worker/worker.ts k.ts --format cjs,esm --dts",
    "dev": "tsup google.ts fontface.ts fontface-dom.ts typr/index.ts parse/index.ts parser-worker/index.ts parser-worker/worker.ts k.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^22"
  }
}
