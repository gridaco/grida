{
  "name": "@grida/tailwindcss-colors",
  "version": "4.0.1",
  "private": false,
  "description": "Tailwind CSS color data package - provides color values in multiple formats (RGBA, HEX, OKLCH) for programmatic use. Not intended for direct CSS import in production.",
  "keywords": [
    "colors",
    "data",
    "grida",
    "tailwind",
    "tailwindcss"
  ],
  "homepage": "https://grida.co",
  "license": "MIT",
  "author": "softmarshmallow",
  "repository": "https://github.com/gridaco/grida",
  "files": [
    "css",
    "json",
    "README.md"
  ],
  "exports": {
    "./css/rgb.css": "./css/rgb.css",
    "./css/rgba.css": "./css/rgba.css",
    "./css/hex.css": "./css/hex.css",
    "./css/oklch.css": "./css/oklch.css",
    "./json/rgb.json": "./json/rgb.json",
    "./json/rgbf.json": "./json/rgbf.json",
    "./json/rgba.json": "./json/rgba.json",
    "./json/hex.json": "./json/hex.json",
    "./json/oklch.json": "./json/oklch.json"
  },
  "scripts": {
    "generate": "deno run --allow-read --allow-write --allow-net --allow-env --no-prompt scripts/generate.ts",
    "prepublishOnly": "pnpm generate"
  }
}
